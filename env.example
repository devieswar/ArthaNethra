# ArthaNethra Environment Variables
# Copy this file to .env and fill in your actual values

# LandingAI ADE API
LANDINGAI_API_KEY=your_landingai_api_key_here
LANDINGAI_API_URL=https://api.landing.ai/v1

# AWS Bedrock
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# Database (Weaviate)
WEAVIATE_URL=http://weaviate:8080
WEAVIATE_API_KEY=

# Database (Neo4j - Optional)
NEO4J_URI=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=arthanethra123

# Application Settings
DEBUG=True
API_PREFIX=/api/v1
CORS_ORIGINS=http://localhost:4200,http://localhost:3000
MAX_UPLOAD_SIZE=104857600

# Storage
UPLOAD_DIR=./uploads
CACHE_DIR=./cache

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/arthanethra.log

# JWT (Optional - for future auth)
JWT_SECRET=your_jwt_secret_here
JWT_ALGORITHM=HS256

